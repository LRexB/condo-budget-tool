{
  "name": "condo-repair-manager",
  "version": "1.0.0",
  "description": "Condominium repair management system with spreadsheet import and detailed tracking",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["condominium", "repairs", "property-management", "spreadsheet"],
  "author": "Condo Repair Manager",
  "license": "MIT"
}